{
  "name": "application",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rimraf node_modules/@types/bull/node_modules && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf node_modules/@types/bull/node_modules && rimraf dist && nest start --watch",
    "migration:generate-production": "export NODE_ENV=production && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/migrations -n refactor",
    "migration:generate-development": "export NODE_ENV=development && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/migrations -n refactor",
    "migration:create-production": "export NODE_ENV=production && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create -d src/migrations -n refactor",
    "schema:drop-production": "export NODE_ENV=production && export NODE_ENV=production && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js schema:drop",
    "migration:revert-production": "export NODE_ENV=production && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert",
    "migration:run-production": "export NODE_ENV=production && ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "webpack:hrm": "webpack --config webpack.config.js --watch --progress",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/event-emitter": "^0.0.2",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.10",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.13",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "aws-sdk": "^2.826.0",
    "bcrypt": "^5.0.0",
    "bull": "^3.20.1",
    "bull-board": "^1.3.0",
    "bullmq": "^1.14.7",
    "casbin": "^5.2.3",
    "class-transformer": "0.3.1",
    "class-validator": "^0.13.0",
    "compression": "^1.7.4",
    "consola": "^2.15.0",
    "dayjs": "^1.10.4",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "email-templates": "^8.0.3",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.2.3",
    "firebase-admin": "^9.5.0",
    "helmet": "^4.3.1",
    "html-to-text": "^7.0.0",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nest-winston": "^1.4.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cron": "^1.7.2",
    "@types/ejs": "^3.0.5",
    "@types/email-templates": "^8.0.1",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
